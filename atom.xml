---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>{{ site.title }}</title>
 <link href="http://blog.tang3w.com/atom.xml" rel="self"/>
 <link href="http://blog.tang3w.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://blog.tang3w.com/</id>
 <author>
   <name>唐天勇</name>
   <email>tang3w@gmail.com</email>
 </author>

 {% for post in site.posts limit:20 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://blog.tang3w.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://blog.leezhong.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
